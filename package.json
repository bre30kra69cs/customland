{
  "name": "customland",
  "version": "0.0.1",
  "description": "Customland it is js ecosystem with zero dependencies",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run build && node dist",
    "format": "npx prettier --write packages",
    "lint": "npx eslint packages --ext .ts",
